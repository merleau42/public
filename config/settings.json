{
    "http.proxySupport": "off",
    "terminal.integrated.localEchoLatencyThreshold": -1,
    "terminal.integrated.localEchoEnabled": "off",
    "terminal.integrated.fontSize": 13,
    "workbench.startupEditor": "none",
    "editor.fontFamily": "JetBrains Mono, Menlo, Monaco, Courier New, monospace",
    "editor.fontWeight": "normal",
    "editor.fontSize": 13,
    "editor.lineHeight": 1.5,
    "editor.letterSpacing": 0,
    "debug.console.fontSize": 13,
    "window.commandCenter": true,
    "workbench.preferredDarkColorTheme": "Google Dark",
    "workbench.preferredLightColorTheme": "Google Light",
    "workbench.iconTheme": "monospace",
    "window.autoDetectColorScheme": true,
    "workbench.layoutControl.enabled": true,
    "inlineChat.mode": "preview",
    "workbench.activityBar.location": "top",
    "window.menuBarVisibility": "classic",
    "editor.minimap.enabled": false,
    "redhat.telemetry.enabled": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "IDX.aI.geminiApiKey": "AIzaSyD7oVbWhEhqsOkMoh2dtKYoGkOGv_AMeBQ",
    "IDX.aI.geminiModel": "gemini-2.5-pro",
    "IDX.aI.modelProvider": "gemini-api",
    "better-comments.tags": [
      {
        "tag": "!",
        "color": "yellow",
        "strikethrough": false,
        "underline": false,
        "backgroundColor": "transparent",
        "bold": false,
        "italic": false
      }
    ],
    "statusbar_command.commands": [
      {
        "id": "sbc_settings",
        "name": "settings",
        "text": "$(gear)",
        "tooltip": "환경설정",
        "command": "workbench.action.openWorkspaceSettings",
        "alignment": "left",
        "priority": 100,
      },
      {
        "id": "push_all",
        "name": "push_all",
        "text": "▲DEV",
        "command": "workbench.action.tasks.runTask",
        "arguments": ["push_all"],
        "priority": 99,
      },    
      {
        "id": "sbc_format",
        "name": "formatDocument",
        "text": "$(file-text)",
        "tooltip": "format",
        "command": "editor.action.formatDocument",
        "include": "\\.[js|json]",
        "alignment": "left",
        "priority": 1,
      },
