{
	"editor.minimap.enabled": false,
	"actionButtons": {

		"reloadButton": "reload",
		"defaultColor": "lime",
		"loadNpmCommands": false,
		"commands": [
			{
				"name": "settings.json",
				"color": "lime",
				"useVsCodeApi": true,
				"command": "workbench.action.openSettingsJson"
			},
			{
				"name": "예제 채점",
				"color": "yellow",
				"useVsCodeApi": true,
				"command": "bojhelper.test"
			},
			{
				"name": "g++",
				"color": "yellow",
				"command": "g++ ${file} -o ./.vscode/cppexe; clear; ./.vscode/cppexe; echo '\nhttps://www.acmicpc.net/submit/'${fileBasenameNoExtension}"
            },
			{
				"name": "실험실",
				"color": "gray",
				"useVsCodeApi": true,
				"command": ""
			}
		],
		"customVars": {
			"port": "COM4"
		}
	},
	"window.titleBarStyle": "custom",
	"window.commandCenter": true,
	"editor.mouseWheelZoom": true,
	"terminal.integrated.tabs.location": "left",
	"terminal.integrated.tabs.enabled": false,
	"BOJ.author": "1",
	"BOJ.extension": "1",
	"csv-edit.disableBorders": true,
	"csv-edit.fontSizeInPx": 12,
	"csv-edit.doubleClickColumnHandleForcedWith": 40,
	"window.zoomLevel": -0.5
}